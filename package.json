{
  "name": "digitalexplanation",
  "version": "1.0.0",
  "description": "Tema Digital Explanation basado en Storefront",
  "main": "index.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --progress",
    "dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
    "clean": "rm -rf ./build/* ./assets/build/*",
    "svg": "svgr -d ./assets/src/js/icons ./assets/src/icons",
    "lint": "eslint ./assets/src/js/*",
    "lint:fix": "eslint --fix ./assets/src/js/*",
    "lint-css": "stylelint ./assets/src/sass/**/**/*.scss",
    "lint-css:fix": "npm run lint-css -- --fix",
    "precommit": "npm rin lint:fix && npm run lint-css:fix && npm run build"
  },
  "keywords": [
    "wordpress",
    "child-theme",
    "theme-storefront"
  ],
  "author": "Juan Carlos Jiménez Gutiérrez <jcjimenez29@misena.edu.co>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@beyonk/google-fonts-webpack-plugin": "^1.5.0",
    "@svgr/cli": "^5.5.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.17.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsdoc": "^30.7.13",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass-loader": "^10.1.0",
    "sass-mq": "^5.0.1",
    "stylelint": "^13.8.0",
    "stylelint-config-wordpress": "^17.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  }
}
